        <Field Name="medicineBill">
          <DataField>medicineBill</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ShripadaSource</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        MedicineOrder.medicineName, MedicineOrder.quantity, MedicineOrder.totalAmount, visitData.medicineBill
FROM            MedicineOrder INNER JOIN
                         visitData ON MedicineOrder.visitID = visitData.srNo
WHERE        (MedicineOrder.visitID = @visitID)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@visitID">
            <Value>=Parameters!visitID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
