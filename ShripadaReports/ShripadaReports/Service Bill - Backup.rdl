        </Field>
        <Field Name="dateOfAdmission">
          <DataField>dateOfAdmission</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ShripadaSource</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        ServiceOrder.ServiceName, ServiceOrder.noOfDays, ServiceOrder.totalAmount, Patients.patientID, Patients.patientName, visitData.dateOfAdmission
FROM            ServiceOrder INNER JOIN
                         Patients ON ServiceOrder.patientID = Patients.patientID INNER JOIN
                         visitData ON ServiceOrder.visitID = visitData.srNo AND Patients.patientID = visitData.patientID
WHERE        (ServiceOrder.visitID = @visitID)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@visitID">
            <Value>=Parameters!visitID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
