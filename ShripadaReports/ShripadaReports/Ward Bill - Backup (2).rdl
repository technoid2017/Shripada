        <CommandType>Text</CommandType>
        <CommandText>SELECT        WardOrder.patientID, WardOrder.wardType, WardOrder.admissionDate, WardOrder.dischargeDate, WardOrder.noOfdays, WardOrder.stayAmount, WardOrder.operationAmount, WardOrder.aneasthesiaAmount, 
                         WardOrder.oTChargeAmount, WardOrder.assistantAmount, WardOrder.nursingAmount, WardOrder.paediatricianAmount, WardOrder.roundAmount, WardOrder.miscellaneousAmount, Patients.patientName
FROM            WardOrder INNER JOIN
                         Patients ON WardOrder.patientID = Patients.patientID
WHERE        (WardOrder.visitID = @visitID)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@visitID">
